<h1>Welcome to the Site!</h1>


<% if user_signed_in? %>
<p>
  Your email is:
  <%= current_user.email %><br />
  Your id is:
  <%= current_user.id %><br />
  <%= link_to("Sign Out", destroy_user_session_path, method: :delete) %>
</p>
<% else %>
<p>
  <%= link_to("Sign Up", new_user_registration_path) %>
  <%= link_to("Login", new_user_session_path) %>
</p>
<% end %>